    "@types/whatwg-fetch": "0.0.33",
    "axios": "^0.15.3",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.15",
    "copy-webpack-plugin": "^4.0.1",
    "html-webpack-plugin": "^2.26.0",
    "mocha": "^3.2.0",
    "mocha-typescript": "^1.0.18",
    "sinon": "^1.17.7",
    "source-map-loader": "^0.1.6",
    "ts-loader": "^2.0.0",
    "ts-node": "^2.0.0",
    "ts-url": "git+https://github.com/atothek1/ts-url.git",
    "tslint": "^4.4.2",
