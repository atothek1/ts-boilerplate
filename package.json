{
  "name": "ts-boilerplate",
  "version": "1.0.0",
  "description": "This a base empty Project setup as starting point for fresh TypeScript projects.",
  "main": "./app/src/Index.ts",
  "scripts": {
    "test": "mocha --growl --opts config/mocha.opts",
    "build": "npm run build:dev",
    "build:dev": "npm run test && webpack --config config/webpack.development.js",
    "build:prod": "npm run test && webpack --config config/webpack.production.js",
    "webpack": "webpack --config config/webpack.development.js",
    "webpack:watch": "webpack --config config/webpack.development.js --watch"
  },
